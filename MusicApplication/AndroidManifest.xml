<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musicapplication"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="14" />

    <application
        android:name="com.music.Application.musicApp"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Sherlock" >
        <activity
            android:name="com.example.musicapplication.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.musicapplication.SongsActivity"
            android:label="@string/songs"
            android:theme="@style/Theme.Sherlock" >
        </activity>
        <activity
            android:name="com.example.musicapplication.DirectoriesActivity"
            android:label="@string/title_activity_list_directories" >
        </activity>
        <activity
            android:name="com.example.musicapplication.StreamMediaActivity"
            android:label="@string/title_activity_stream_media" >
        </activity>
    </application>

</manifest>